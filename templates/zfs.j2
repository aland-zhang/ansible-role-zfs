[zfs-source]
name = ZFS on Linux for EL{{ ansible_distribution_major_version }} - Source
baseurl = http://download.zfsonlinux.org/epel/{{ ansible_distribution_version }}/SRPMS/
enabled = 0
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux

[zfs-testing]
name = ZFS on Linux for EL{{ ansible_distribution_major_version }} - dkms - Testing
baseurl = http://download.zfsonlinux.org/epel-testing/{{ ansible_distribution_version }}/$basearch/
enabled = 0
metadata_expire = 7d
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux

[zfs-testing-kmod]
name = ZFS on Linux for EL{{ ansible_distribution_major_version }} - kmod - Testing
baseurl = http://download.zfsonlinux.org/epel-testing/{{ ansible_distribution_version }}/kmod/$basearch/
enabled = 0
metadata_expire = 7d
gpgcheck = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-zfsonlinux

